{
  "title": "options",
  "description": "post数据任务",
  "type": "object",
  "properties": {
    "post_url": {
      "title": "请求链接",
      "examples": [
        "www.baidu.com"
      ],
      "type": "string"
    },
    "sslpv": {
      "title": "ssl版本",
      "enum": [
        "",
        "TLSv1",
        "TLSv1.1",
        "TLSv1.2",
        "SSLv3"
      ],
      "type": "string",
      "default": ""
    },
    "disable_ssl_verification": {
      "type": "boolean",
      "default": false
    },
    "headers": {
      "title": "请求头信息",
      "type": "object"
    },
    "contentType": {
      "type": "string"
    },
    "data": {
      "title": "请求数据",
      "type": "object"
    }
  },
  "required": [
    "post_url",
    "data"
  ]
}
